<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tarea56</title>
    <script src="https://kit.fontawesome.com/3c52229e0a.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="~/materialize/css/materialize.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav>
            <div class="nav-wrapper ">
                <div class="main">

                    <ul class="">
                        <li class="">
                            <a class="logo-a " asp-area="" asp-controller="Vehiculos" asp-action="Index">VEHICULOS</a>
                        </li>
                        <li class="right">
                            <partial name="_LoginPartial" />
                        </li>
                        <li class="right">
                            <a class=" waves-effect waves-light light-blue btn" asp-area="" asp-controller="Historico" asp-action="Index">Liberados</a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>


    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <!--
        <footer class="page-footer">
            <div class="container">
                &copy; 2020 - Tarea56 - <a  asp-area="" asp-controller="Home" asp-action="Privacy">- Jesus Daniel -</a>
            </div>
        </footer>
    -->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/materialize/js/materialize.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
